{
    "name": "gateway",
    "version": "1.0.0",
    "description": "gateway",
    "main": "server.js",
    "scripts": {
        "start": "NODE_ENV=dev nodemon ./server/server.js --exec babel-node",
        "build-server": "babel server -d build",
        "aws:clean": "rm -rf build && mkdir build",
        "aws:download": "rm -rf build/controllers/download && mkdir build/controllers/download",
        "aws:build": "npm run aws:clean && npm run build-server ",
        "aws:start": "NODE_ENV=prod node ./build/server.js",
        "test": "mocha server/**/*.test.js",
        "test-watch": "nodemon --exec 'npm test'"
    },
    "engines": {
        "node": ">=v8.4.0"
    },
    "keywords": [],
    "author": "Asif",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.17.2",
        "express": "^4.15.4",
        "express-http-proxy": "^1.0.7",
        "helmet": "^3.8.1",
        "lodash": "^4.17.4",
        "mongoose": "^4.11.9",
        "morgan": "^1.8.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^7.2.3",
        "babel-preset-es2015": "^6.24.1",
        "eslint": "^4.5.0",
        "eslint-plugin-node": "^5.1.1",
        "expect": "^1.20.2",
        "mocha": "^3.5.0",
        "nodemon": "^1.11.0"
    }
}
